(rule $NP (bike) (ConstantFn (fb:type.object.type fb:object.bike)))
(rule $RelNP (price) (ConstantFn fb:object.bike.price))
(rule $RelNP (id)    (ConstantFn fb:object.bike.id))
(rule $RelNP (color) (ConstantFn fb:object.bike.color))
(rule $RelNP (size)  (ConstantFn fb:object.bike.size))

(rule $NP ($RelNP of $NP) (lambda r (lambda x ((reverse (var r)) (var x)))))
(rule $ROOT ($NP) (IdentityFn))
(rule $ROOT ($NP with the largest $RelNP) (lambda x (lambda y (argmax 1 5 (var x) (var y)))))
(rule $ROOT ($NP with the smallest $RelNP) (lambda x (lambda y (argmax 1 5 (var x) (var y)))))